post:
  description: 登录
  body:
    username:
      description: 用户名
      type: string
      required: true
      example: abc
    password:
      description: 密码
      type: string
      required: true
      example: abc
  (runner):
    after: ./runner/afterLogin.js
  responses:
    200:
      body:
        type: string
        example: fdafda232432fdaxfda25dfa
